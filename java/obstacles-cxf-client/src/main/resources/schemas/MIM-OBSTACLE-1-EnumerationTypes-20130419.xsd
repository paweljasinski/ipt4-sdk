  <simpleType name="DemolitionStatusCode">
    <annotation>
      <documentation xml:lang="en">The status of an Object destined for demolition.</documentation>
      <appinfo>
        <SemanticId>BusinessObjects::Obstacle::Enumerations::DemolitionStatusCode</SemanticId>
      </appinfo>
    </annotation>
    <restriction base="token">
      <enumeration value="ABNDIN">
        <annotation>
