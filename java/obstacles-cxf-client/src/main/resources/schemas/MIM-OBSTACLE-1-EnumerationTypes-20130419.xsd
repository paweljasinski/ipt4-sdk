            <SemanticId>BusinessObjects::Obstacle::Enumerations::DemolitionStatusCode.CANCLD</SemanticId>
            <externalSource>ATCCIS</externalSource>
          </appinfo>
        </annotation>
      </enumeration>
      <enumeration value="EXECTD">
        <annotation>
          <documentation xml:lang="en">The object is demolished.</documentation>
          <appinfo>
            <DisplayValue>Executed</DisplayValue>
            <SemanticId>BusinessObjects::Obstacle::Enumerations::DemolitionStatusCode.EXECTD</SemanticId>
            <externalSource>ATCCIS</externalSource>
          </appinfo>
        </annotation>
      </enumeration>
      <enumeration value="PLNPRL">
        <annotation>
          <documentation xml:lang="en">The object of the demolition is planned as a preliminary demolition; execution can proceed without reference to an authorised commander.</documentation>
          <appinfo>
            <DisplayValue>Planned preliminary</DisplayValue>
            <SemanticId>BusinessObjects::Obstacle::Enumerations::DemolitionStatusCode.PLNPRL</SemanticId>
            <externalSource>ATCCIS</externalSource>
          </appinfo>
        </annotation>
      </enumeration>
      <enumeration value="PLNRES">
        <annotation>
          <documentation xml:lang="en">The object of the demolition is planned as a reserve demolition; execution is to be ordered by a specific authorised commander.</documentation>
          <appinfo>
            <DisplayValue>Planned reserve</DisplayValue>
            <SemanticId>BusinessObjects::Obstacle::Enumerations::DemolitionStatusCode.PLNRES</SemanticId>
            <externalSource>ATCCIS</externalSource>
          </appinfo>
        </annotation>
      </enumeration>
      <enumeration value="PRPEXE">
        <annotation>
          <documentation xml:lang="en">The object is prepared for demolition.</documentation>
          <appinfo>
            <DisplayValue>Prepared for execution</DisplayValue>
            <SemanticId>BusinessObjects::Obstacle::Enumerations::DemolitionStatusCode.PRPEXE</SemanticId>
            <externalSource>ATCCIS</externalSource>
          </appinfo>
        </annotation>
      </enumeration>
      <enumeration value="STATE1">
        <annotation>
          <documentation xml:lang="en">The object is at demolition state 1 (safe). The demolition devices are installed but not armed.</documentation>
          <appinfo>
            <DisplayValue>State 1</DisplayValue>
            <SemanticId>BusinessObjects::Obstacle::Enumerations::DemolitionStatusCode.STATE1</SemanticId>
