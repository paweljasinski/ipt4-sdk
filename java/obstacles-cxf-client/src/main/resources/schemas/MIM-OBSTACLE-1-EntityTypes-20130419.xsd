						<SemanticId>BusinessObjects::Obstacle::Classes::Entity::Symbology.symbolAnnotationText</SemanticId>
						<maximumLength>20</maximumLength>
						<minimumLength>0</minimumLength>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</complexType>
