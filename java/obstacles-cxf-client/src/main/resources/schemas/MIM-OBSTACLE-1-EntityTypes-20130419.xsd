			<documentation xml:lang="en">An Object that is built, installed or established to serve some particular purpose and is identified by the service it provides rather than by its content.</documentation>
			<appinfo>
				<SemanticId>BusinessObjects::Obstacle::Classes::Facility::Facility</SemanticId>
			</appinfo>
		</annotation>
		<complexContent>
			<extension base="mim:Object">
				<sequence>
					<element name="operationalStatusCode" type="mim:FacilityOperationalStatusCode">
						<annotation>
							<documentation xml:lang="en">The operational status of a Facility.</documentation>
							<appinfo>
								<SemanticId>BusinessObjects::Obstacle::Classes::Facility::Facility.operationalStatusCode</SemanticId>
								<externalSource>ATCCIS</externalSource>
							</appinfo>
						</annotation>
					</element>
					<element name="demolitionStatusCode" type="mim:DemolitionStatusCode" minOccurs="0">
						<annotation>
							<documentation xml:lang="en">The status of a Facility destined for demolition.</documentation>
							<appinfo>
								<SemanticId>BusinessObjects::Obstacle::Classes::Facility::Facility.demolitionStatusCode</SemanticId>
								<externalSource>ATCCIS</externalSource>
							</appinfo>
						</annotation>
					</element>
					<element name="operationalStatusQualifierCode" type="mim:FacilityOperationalStatusQualifierCode" minOccurs="0">
						<annotation>
							<documentation xml:lang="en">The qualification of the operational status of a Facility.</documentation>
							<appinfo>
								<SemanticId>BusinessObjects::Obstacle::Classes::Facility::Facility.operationalStatusQualifierCode</SemanticId>
								<externalSource>ATCCIS</externalSource>
							</appinfo>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
